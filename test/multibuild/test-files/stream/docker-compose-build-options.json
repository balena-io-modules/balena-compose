{
  "version": "2.4",
  "services": {
    "network": {
      "build": {
        "context": "./network",
        "network": "none"
      }
    },
    "extra_hosts": {
      "build": {
        "context": "./extra_hosts",
        "extra_hosts": {
          "foo": "8.8.8.8"
        }
      }
    },
    "target": {
      "build": {
        "context": "./target",
        "target": "stage1"
      }
    },
    "cache_from": {
      "build": {
        "context": "./cache_from",
        "cache_from": ["alpine:latest"]
      }
    }
  }
}
